<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
		"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<t:layout title="title" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
	<t:parameter name="subMenuBlock">
		<ul>
			<ul id="local">
				<li>
					<t:pagelink t:page="Edit" context="editPageContext">${message:org.trails.i18n.edit}</t:pagelink>
				</li>
				<li>
					<t:actionlink class="deletelink" t:id="delete">${message:org.trails.i18n.remove}</t:actionlink>
				</li>
			</ul>
		</ul>
	</t:parameter>
	<t:parameter name="navBlock">
		<a style="font-weight:bolder;margin-left:10px" t:id="link" page="List" context="classDescriptor.type">${listAllLinkMessage}</a>
	</t:parameter>

	<h1>${title}</h1>

	<br/>


	<t:beandisplay object="bean" model="beanModel"/>

</t:layout>